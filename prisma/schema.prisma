generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model services {
  id String @unique @default(uuid())
  type String 
  tags String[]
  title String
  shortDesc String
  desc String
  icon String
  styles String
}
